   "outputs": [],
   "source": [
    "lr = LinearRegression()\n",
    "predicted2 = lr.fit(X,y)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "id": "af82df23-da61-46c6-be20-45193f7e7470",
